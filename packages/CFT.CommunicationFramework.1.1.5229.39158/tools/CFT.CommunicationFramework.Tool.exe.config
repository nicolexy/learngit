<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  
  <appSettings>
    <!--RelayServer Configuration-->

    <add key="relayServerIP" value="172.25.38.123" />
    <add key="relayServerPort" value="22000" />
    <add key="RelayDefaultSPId" value="2000000000" />
    <!--ICE Server Configuration-->
    <add key="ICEServerIP" value="172.25.39.53" />
    <add key="ICEServerPort" value="6644" />
    <add key="ICEUsername" value="rayguo" />
    <add key="ICEPassword" value="1111" />

    <add key="TraceCF" value="true" />
    
    <!--log4net-->
    <!--<add key="log4net.Config" value="Configs\Log4net.config" />-->
    <!--<add key="log4net.Config.Watch" value="False" />-->
  </appSettings>
  <log4net>
    <appender name="DefaultFileAppender" type="log4net.Appender.RollingFileAppender" >
      <file value="Logs/" />
      <datePattern value="yyyy_MM_dd'.log'" />
      <staticLogFileName value="false" />
      <rollingStyle value="Date" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%ndc] - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="INFO" />
      <appender-ref ref="DefaultFileAppender" />
    </root>
  </log4net>
</configuration>